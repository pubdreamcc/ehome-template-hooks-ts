		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "4.3.0",
		"fs-extra": "^8.1.0",
		"hard-source-webpack-plugin": "^0.13.1",
		"html-webpack-plugin": "4.0.0-beta.11",

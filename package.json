  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run lint",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
